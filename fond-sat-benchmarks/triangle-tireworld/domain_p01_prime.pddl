(define (domain triangle-tire)
  (:requirements :typing :strips :non-deterministic)
  (:types location)
  (:predicates (vehicleat ?loc - location) (spare-in ?loc - location) (road ?from - location ?to - location) (not-flattire) (turnDomain) (q1 ?loc06 - location) (q2 ?loc06 - location) (q3 ?loc06 - location))
  (:action move-car
    :parameters (?from - location ?to - location)
    :precondition (and (vehicleat ?from) (road ?from ?to) (not-flattire) (turnDomain))
    :effect (and (oneof (and (vehicleat ?to) (not (vehicleat ?from))) (and (vehicleat ?to) (not (vehicleat ?from)) (not (not-flattire)))) (not (turnDomain)))
  )
  (:action changetire
    :parameters (?loc - location)
    :precondition (and (spare-in ?loc) (vehicleat ?loc) (turnDomain))
    :effect (and (not (spare-in ?loc)) (not-flattire) (not (turnDomain)))
  )
  (:action trans-1
    :parameters (?loc06 - location)
    :precondition (and (or (and (q1 ?loc06) (not (vehicleat ?loc06))) (and (q2 ?loc06) (not (vehicleat ?loc06)))) (not (turnDomain)))
    :effect (and (q2 ?loc06) (not (q1 ?loc06)) (not (q3 ?loc06)) (turnDomain))
  )
  (:action trans-2
    :parameters (?loc06 - location)
    :precondition (and (or (and (q1 ?loc06) (vehicleat ?loc06)) (and (q2 ?loc06) (vehicleat ?loc06)) (q3 ?loc06)) (not (turnDomain)))
    :effect (and (q3 ?loc06) (not (q1 ?loc06)) (not (q2 ?loc06)) (turnDomain))
  )
)